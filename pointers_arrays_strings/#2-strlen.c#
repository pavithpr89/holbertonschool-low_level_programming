#include <unistd.h>
#include "main.h"

/**                                                                                                                                                 
 * _putchar - writes a character to stdout                                                                                                          
 * @c: character to print                                                                                                                           
 *                                                                                                                                                  
 * Return: 1 on success, -1 on error                                                                                                                
 */
int _putchar(char c)
{
return write(1, &c, 1);
}#include "main.h"

/**
 * _strlen - returns the length of a string
 * @s: pointer to the string
 *
 * Return: the number of characters in the string
 */
int _strlen(char *s)
{
int length = 0;

while (s[length] != '\0')
length++;

return (length);
}
